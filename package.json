{
	"name": "reactcms",
	"version": "1.0.0",
	"description": "react cms",
	"main": "webpack.config.js",
	"scripts": {
		"dev-node": "set NODE_ENV=dev&&node build/webpack.dev.config.js",
		"dev": "set NODE_ENV=dev&&webpack-dev-server --config build/webpack.config.js --content-base public/ --host localhost --port 9000 --inline --history-api-fallback ",
		"dev-mac": "NODE_ENV=dev webpack-dev-server --config build/webpack.config.js --content-base public/ --host localhost --port 9000 --open --inline --history-api-fallback ",
		"build": "set NODE_ENV=prod&&webpack -p --config build/webpack.config.js",
		"build-mac": "NODE_ENV=prod webpack -p --config build/webpack.config.js"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"babel-core": "^6.26.0",
